{
    "name": "todora/todora",
    "type": "project",
    "description": "Another Todo app named Todora",
    "license": "MIT",
    "autoload": {
        "psr-4": { "Todora\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Todora\\Tests\\": "tests/",
            "Todora\\Fixtures\\": "fixtures/"
        },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=7.2.0",
        "php-64bit": ">=7.2.0",
        "ext-apcu": ">=5.0.0",
        "ext-pdo_pgsql": ">=7.2.0",
        "ext-pgsql": ">=7.2.0",
        "beberlei/assert": "^2.7",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "egulias/email-validator": "^2.1",
        "league/tactician-bundle": "^1.1",
        "league/tactician-doctrine": "^1.1",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.6",
        "ramsey/uuid-doctrine": "^1.3",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.0",
        "snc/redis-bundle": "^2.0",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/symfony": "3.4.*",
        "twig/twig": "^2.0",
        "zendframework/zend-diactoros": "^1.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "fzaninotto/faker": "^1.6",
        "matthimatiker/opcache-bundle": "^0.7",
        "pds/skeleton": "^1.0",
        "phpunit/phpunit": "^6.0",
        "pixers/doctrine-profiler-bundle": "^1.0",
        "sensio/generator-bundle": "^3.0",
        "symfony/dotenv": "^3.4",
        "symfony/phpunit-bridge": "^3.4"
    },
    "scripts": {
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "tests:all": "./vendor/bin/phpunit --colors=always",
        "tests:unit": "./vendor/bin/phpunit --colors=always --testsuite=unit-tests",
        "tests:functional": "./vendor/bin/phpunit --colors=always --testsuite=functional-tests"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative"
    }
}
