{
    "type": "project",
    "license": "MIT",
    "description": "Todora app",
    "require": {
        "php": ">=7.2.0",
        "php-64bit": ">=7.2.0",
        "ext-apcu": ">=5.0.0",
        "ext-iconv": "*",
        "ext-mbstring": ">=7.2.0",
        "ext-pdo_pgsql": ">=7.2.0",
        "beberlei/assert": "^2.8",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/orm": "^2.5.11",
        "egulias/email-validator": "^2.1",
        "league/tactician-bundle": "^1.1",
        "league/tactician-doctrine": "^1.1",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.7",
        "ramsey/uuid-doctrine": "^1.4",
        "snc/redis-bundle": "^2.0",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/security-csrf": "^4.0",
        "symfony/templating": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.2",
        "dama/doctrine-test-bundle": "^5.0",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "fzaninotto/faker": "^1.7",
        "matthimatiker/opcache-bundle": "^0.7",
        "pds/skeleton": "^1.0",
        "pixers/doctrine-profiler-bundle": "^1.0",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug-bundle": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^4.0",
        "symfony/web-server-bundle": "^4.0",
        "theofidry/alice-data-fixtures": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.2.1"
        }
    },
    "autoload": {
        "psr-4": {
            "Todora\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Todora\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "todora:migrate:test": [
            "@php vendor/bin/phinx migrate -e test --ansi"
        ],
        "todora:migrate:dev": [
            "@php vendor/bin/phinx migrate -e dev --ansi"
        ],
        "todora:cs-fix": [
            "@php vendor/bin/php-cs-fixer fix -v --diff --ansi"
        ],
        "todora:analyze": [
            "@php bin/console doctrine:schema:validate --ansi --skip-sync",
            "@php bin/console lint:twig resources/templates --ansi",
            "@php bin/console lint:yaml config/ --ansi",
            "@php vendor/bin/phinx test --ansi",
            "@php vendor/bin/parallel-lint src tests",
            "@php vendor/bin/php-cs-fixer fix -v --diff --dry-run --ansi",
            "@php vendor/bin/phpmd src,tests text phpmd-ruleset.xml",
            "@php vendor/bin/phpcpd src tests",
            "@php vendor/bin/phpstan analyse -l 7 -c phpstan.neon --no-progress --ansi src tests"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C10NFZ58ZFRX8P52RTNPXNB0",
            "allow-contrib": false
        }
    }
}
